[
    {
      "id": "errorInformation.errorCode",
      "pattern": "600[1-9]"
    },
    {
      "id": "errorInformation.errorDescription",
      "pattern": "This is a mock error description"
    },
    {
      "id": "accountRequestId",
      "pattern": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
    },
    {
      "id": "authChannel",
      "pattern": "OTP"
    },
    {
        "id": "authToken",
        "pattern": "[0-9]{6}"
    },
    {
        "id": "authUri",
        "pattern": "https://auth-server:3000/auth"
    },
    {
        "id": "callbackUri",
        "pattern": "https://auth-server:3000/callback"
    },
    {
        "id": "accounts.accountNickname",
        "pattern": "^(?!\\s*$)[\\w .,'-]{1,128}$"
    },
    {
        "id": "accounts.address",
        "pattern": "^([0-9A-Za-z_~\\-\\.]+[0-9A-Za-z_~\\-])$"
    }
    
]