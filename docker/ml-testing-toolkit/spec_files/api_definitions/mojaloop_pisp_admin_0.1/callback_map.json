{
    "/tppAccountRequest": {
      "post": {
        "fspid": "{$request.headers.fspiop-source}",
        "successCallback": {
          "method": "put",
          "path": "/tppAccountRequest/{ID}",
          "pathPattern": "/tppAccountRequest/{$request.body.accountRequestId}",
          "headerOverride": {
            "FSPIOP-Source": "{$request.headers.fspiop-destination}",
            "FSPIOP-Destination": "{$request.headers.fspiop-source}",
            "Content-Type": "{$session.negotiatedContentType}",
            "Date": "{$request.headers.date}"
          }
        },
        "errorCallback": {
          "method": "put",
          "path": "/tppAccountRequest/{ID}/error",
          "pathPattern": "/tppAccountRequest/{$request.body.accountRequestId}/error",
          "headerOverride": {
            "FSPIOP-Source": "{$request.headers.fspiop-destination}",
            "FSPIOP-Destination": "{$request.headers.fspiop-source}",
            "Content-Type": "{$session.negotiatedContentType}",
            "Date": "{$request.headers.date}"
          }
        }
      }
    }
}
  